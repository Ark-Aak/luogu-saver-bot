{
  "compilerOptions": {
    "target": "es2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
	"paths": {
	  "@/*": ["src/*"]
	},
	"baseUrl": "."
  },
  "include": ["src"],
}
